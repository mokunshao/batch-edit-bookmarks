{
  "manifest_version": 2,
  "name": "Batch Edit Bookmarks",
  "version": "0.0.1",
  "description": "Batch Edit Bookmarks",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "bookmarks"
  ]
}
